<div id="disqus_thread"></div>
<script>
var disqus_config = function () {
  this.page.identifier = <%= card_path(@card) %>;
  // to_s is because of weird errors that happened locally with an edgecase that was somehow not passing string.
  this.page.title = <%= @card.name.to_s %>;
};

(function() {  // DON'T EDIT BELOW THIS LINE
  var d = document, s = d.createElement('script');

  s.src = '//devsdeck.disqus.com/embed.js';

  s.setAttribute('data-timestamp', +new Date());
  (d.head || d.body).appendChild(s);
})();
</script>
